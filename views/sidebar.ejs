<div class="side-bar">
  <a href="/"><h1 class="default-color" >Vault | Tage</h1></a>

  <div class="upload-container">
    <label for="file-input" class="default-btn default " >Upload File</label>
    <form
      method="POST"
      enctype="multipart/form-data"
      action="/upload/files<%=folderId ? "/"+folderId : ''%>"
      id="form-upload-file"
    >
      <input
        type="file"
        class="file-input "
        id="file-input"
        name="file"
      />
      
    </form>
    <span class="default-btn default" id="modal-open" style="text-align: center;">Create Folder</span>
  </div>
  <div style="display: flex; flex-direction: column; gap: 5px">
    <a href="/" class="default-btn default" >Home </a>
    <a href="/sharedfiles" class="default-btn default" >Shared </a>
  </div>
  <a href="/delete">Delete Users</a>
  <form method="POST" action="/logout"><button>Logout</button></form>
</div>
